          "name": "Login - Admin (Get Token)",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('Status 200', () => pm.response.to.have.status(200));",
                  "pm.test('Has token', () => {",
                  "    const data = pm.response.json();",
                  "    pm.expect(data.success).to.be.true;",
                  "    pm.expect(data.data.token).to.be.a('string');",
                  "    pm.collectionVariables.set('token', data.data.token);",
                  "});"
                ]
              }
            }
          ],
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@CosmeticsDB.info\",\n  \"password\": \"@1\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["api", "auth", "login"]
            }
          }
